{
	"compilerOptions": {
	  "target": "ES2020",
	  "module": "ESNext",
	  "moduleResolution": "Bundler",
	  "rootDir": "src",
	  "outDir": "dist",
	  "strict": true,
	  "declaration": true,
	  "declarationMap": false,
	  "composite": true,
	  "esModuleInterop": true,
	  "skipLibCheck": true
	},
	"include": ["src/**/*"],
	"exclude": ["dist"]
  }
  
  